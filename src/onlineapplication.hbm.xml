		<property name="examYear5" column="examYear5"/>
		<property name="subjects5" column="subjects5"/>
		<property name="results5" column="results5"/>
		
		<property name="experienceA" column="experienceA"/>
		<property name="experienceB" column="experienceB"/>
		<property name="experienceC" column="experienceC"/>
		<property name="experienceD" column="experienceD"/>
		<property name="experienceE" column="experienceE"/>
		<property name="experienceF" column="experienceF"/>
		
		<property name="examOption" column="examOption"/>
		<property name="fees" column="fees"/>
	
		
	</class>
</hibernate-mapping>